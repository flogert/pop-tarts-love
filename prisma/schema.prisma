generator client {
  provider = "prisma-client-js"
}

model User {
  id             Int      @id @default(autoincrement())
  email          String   @unique
  password       String
  firstName      String
  lastName       String
  age            Int
  gender         String
  location       String
  education      String?
  maritalStatus  String?
  bio            String?
  interests      String[]
  newsletter     Boolean
  createdAt      DateTime @default(now())
  updatedAt      DateTime @updatedAt

  @@map("User")
}